//#This is modified from compare.src by ray and sherry in summer 2010
//compare according to length first

template <class Item>
int
array<Item>::compare_length(const Item* s1, const Item* s2)
{

if (s1->size()>s2->size())
	return 1;
else if (s1->size()<s2->size())
	return 0;
else
	{
	if (*s1 < *s2)
		return -1;
	if (*s1 == *s2)
		return 0;
	return 1;
	}
}
