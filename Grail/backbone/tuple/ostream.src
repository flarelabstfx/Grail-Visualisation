template <class Input,class Iput, class Output>
std::ostream& 
operator<<(std::ostream& os, const tuple<Input, Iput, Output>& p)
{
	return os << "[" << p.get_left() << "," << p.get_mid() << "," <<
        p.get_right() <<
                "]";
};
